<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>User Id</th>
      <th>User type</th>
      <th>Education</th>
      <th>Experience</th>
      <th>About me</th>
      <th>City</th>
      <th>State</th>
      <th>Github</th>
      <th>Linkedin</th>
      <th>Twitter</th>
      <th>Instagram</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.user_id %></td>
        <td><%= profile.user_type %></td>
        <td><%= profile.education %></td>
        <td><%= profile.experience %></td>
        <td><%= profile.about_me %></td>
        <td><%= profile.city %></td>
        <td><%= profile.state %></td>
        <td><%= profile.github %></td>
        <td><%= profile.linkedin %></td>
        <td><%= profile.twitter %></td>
        <td><%= profile.instagram %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
